<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-zinc-700 px-5 py-8 text-white">
    <div class="cards flex flex-wrap gap-20 ">
        <% user.posts.forEach(function(elem){ %>
           <div class="card gap-3 px-1 mt-3">
            <div class=" w-52 h-40 bg-sky-200 rounded-lg overflow-hidden">
                <img src="/images/uploads/<%= elem.image %>" alt="">
            </div>
            <h5 class="mt-2 text-sm"><%= elem.title %></h5>
        </div>
        <% }) %>
       
        
    </div>
</div>
<% include ./partials/footer.ejs %>